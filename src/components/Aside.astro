---
const { posts } = Astro.props;
---

<div class="p-4">
  <aside>
    <ul class="list-none p-2">
      {
        posts.map((post: { url: string; frontmatter: { title: string } }) => (
          <li>
            <a href={post.url}>{post.frontmatter.title}</a>
          </li>
        ))
      }
    </ul>
  </aside>
</div>
