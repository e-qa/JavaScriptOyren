---
interface Props {
	title: string;
}

const { title } = Astro.props;




---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="JavaScript müsahibə suallarını və cavablarını sadə dərslərlə öyrənin, yeni başlayanlardan tutmuş irəli səviyyələrə qədər. Kodlaşdırma bacarıqlarınızı inkişaf etdirin və növbəti iş müsahibənizə hazırlaşın!" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>
